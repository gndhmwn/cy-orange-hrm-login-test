{
    "compilerOptions": {
      "target": "ES2020",
      "lib": ["ES2020", "DOM"],
      "module": "NodeNext",
      "moduleResolution": "NodeNext",
      "esModuleInterop": true,
      "resolveJsonModule": true,
      "types": ["cypress", "node"],
      "skipLibCheck": true,
      "noEmit": true
    },
    "include": ["cypress/**/*.ts", "cypress/**/*.tsx", "cypress.config.ts"]
  }
  